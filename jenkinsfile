pipeline {
    agent any
    stages {
        stage('Code Checkout') {
            steps {
                git url: 'https://github.com/tonytmv/igp-abc-tech'
            }
        }
        stage ('Code Compile') {
            steps {
                sh 'mvn compile'
            }
        }
        stage ('Code Test') {
            steps {
                sh 'mvn test'
            }
        }
        stage ('Code Packaging') {
            steps {
                sh 'mvn package'
            }
        }
       
    }
}


